# Integration Test Environment Configuration
# Copy this file to .env.test and fill in your credentials for local testing
# This file should NOT be committed to version control

# OpenAI API Key (required for integration tests)
TRANSFLOW_OPENAI_API_KEY=sk_test_xxxx

# Base URL (optional, defaults to https://api.openai.com/v1)
# Examples for local LLM servers:
# - Ollama: http://localhost:11434/v1
# - LocalAI: http://localhost:8080/v1
# - Vllm: http://localhost:8000/v1
TRANSFLOW_OPENAI_BASE_URL=https://api.openai.com/v1

# Model override (optional, defaults to gpt-4o)
# TRANSFLOW_OPENAI_MODEL=gpt-3.5-turbo

# Log level for integration tests
TRANSFLOW_LOG_LEVEL=INFO
